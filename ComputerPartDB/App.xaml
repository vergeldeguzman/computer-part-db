<Application x:Class="ComputerParstDb.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:local="clr-namespace:ComputerParstDb"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        
        <x:Array x:Key="TypeItems" Type="sys:String">
            <sys:String>Monitor</sys:String>
            <sys:String>Keyboard</sys:String>
            <sys:String>Mouse</sys:String>
            <sys:String>Memory</sys:String>
            <sys:String>Power supply</sys:String>
            <sys:String>Motherboard</sys:String>
            <sys:String>Video card</sys:String>
            <sys:String>Storage drive</sys:String>
            <sys:String>Other</sys:String>
        </x:Array>

        <x:Array x:Key="ConditionItems" Type="sys:String">
            <sys:String>Good</sys:String>
            <sys:String>Bad</sys:String>
            <sys:String>Disposed</sys:String>
            <sys:String>Unknown</sys:String>
        </x:Array>

    </Application.Resources>
</Application>
